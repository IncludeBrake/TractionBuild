{
  "name": "zerotoship",
  "version": "1.0.0",
  "description": "AI-powered product studio that validates ideas, builds MVPs, and launches full products",
  "main": "web-server.js",
  "scripts": {
    "dev": "node web-server.js",
    "build": "webpack --mode production",
    "start": "node web-server.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write web-src/",
    "typecheck": "tsc --noEmit",
    "audit": "npm audit --json > repo-tools/out/js_npm_audit.json || exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerotoship/zerotoship.git"
  },
  "keywords": [
    "ai",
    "product-development",
    "mvp",
    "automation",
    "crewai",
    "neo4j",
    "mermaid"
  ],
  "author": "ZeroToShip Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zerotoship/zerotoship/issues"
  },
  "homepage": "https://zerotoship.dev",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "mermaid": "^10.6.1",
    "d3": "^7.8.5",
    "chart.js": "^4.4.0",
    "axios": "^1.6.0",
    "ws": "^8.14.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "eslint": "^9.0.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
